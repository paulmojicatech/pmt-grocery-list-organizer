{
  "npmScope": "paulmojicatech",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "grocery": {
      "tags": []
    },
    "grocery-e2e": {
      "tags": [],
      "implicitDependencies": ["grocery"]
    },
    "grocery-ionic": {
      "tags": []
    },
    "grocery-ionic-e2e": {
      "tags": [],
      "implicitDependencies": ["grocery-ionic"]
    },
    "grocery-shared-business-logic": {
      "tags": []
    }
  }
}
